<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.github.pomaretta.cide.hibernate.dto.Socios">
        <table name="Socios" schema="cide" catalog=""/>
        <attributes>
            <id name="idSocio">
                <column name="id_socio"/>
            </id>
            <basic name="nombre">
                <column name="nombre"/>
            </basic>
            <basic name="apellidos">
                <column name="apellidos"/>
            </basic>
            <basic name="edad">
                <column name="edad"/>
            </basic>
            <basic name="direccion">
                <column name="direccion"/>
            </basic>
            <basic name="telefono">
                <column name="telefono"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
